<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectoasosiacion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoAsosiacion">
        <activity android:name=".Activities.Contacto"></activity>
        <activity android:name=".Activities.Informacion" />
        <activity android:name=".SplashScreen.Splash_Screen_Bienvenida" />
        <activity android:name=".SplashScreen.Splash_Screen_Inicio" />
        <activity android:name=".Activities.InicioSesion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>